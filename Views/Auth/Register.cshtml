@model LoginViewModel

@{ }
<h1>Register</h1>
<form method="post" asp-action="Login" asp-controller="Auth">
    <div>
        <label>Username</label>
        <input asp-for="Username" />
    </div>
    <div>
        <label>Password</label>
        <input asp-for="Password"/>
    </div>
    <input type="submit" value="Sign in"/> 
</form>